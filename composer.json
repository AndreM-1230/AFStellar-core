{
    "name": "afstellar/af-stellar-core",
    "version": "0.9.0",
    "description": "Lightweight MVC core with ORM, Migrations and Query Builder",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andre Fomichev"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "autoload": {
      "psr-4": {
        "App\\Core\\": "src/Core/",
        "App\\Core\\Database\\": "src/Core/Database/",
        "App\\": "app/",
        "App\\Database\\migrations\\" : "app/Database/migrations/"
      }
    },
    "scripts": {
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
